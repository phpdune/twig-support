{
  "name": "dune/twig-support",
  "description": "Twig support for PHP Dune framework!",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Abhishek B",
      "email": "phpdune@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "twig/twig": "^3.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1",
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "Dune\\Support\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dune\\Support\\Tests\\": "tests/"
    }
  }
}
